{
    "name": "phpugl/twitter-bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Configurable Twitter Bootstrap bundle for symfony2",
    "keywords": ["bootstrap", "css", "js"],
    "homepage": "https://github.com/phpugl/PhpuglTwitterBootstrapBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "PHPUGL",
            "email": "info@phpugl.de"
        }
    ],
    "autoload": {
        "psr-0": {
            "Phpugl\\TwitterBootstrapBundle": ""
        }
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.1,<2.3-dev",
        "leafo/lessphp" : "0.3.*"
    },
    "suggest": {
        "twitter/bootstrap": "2.3.*"
    },
    "target-dir": "Phpugl/TwitterBootstrapBundle"
}
